# OPNsense Stats Dashboard Environment Configuration
# Copy this file to .env and update the values

# Event Collector Authentication
EVENT_COLLECTOR_TOKEN=CHANGEME_GENERATE_SECURE_TOKEN

# qBittorrent Monitor User
QBITTORRENT_MONITOR_PASSWORD=CHANGEME

# Sonarr API Keys
SONARR_MAIN_API_KEY=CHANGEME
SONARR_CARTOONS_API_KEY=CHANGEME
SONARR_ANIME_API_KEY=CHANGEME

# Radarr API Keys
RADARR_MAIN_API_KEY=CHANGEME
RADARR_CARTOONS_API_KEY=CHANGEME
RADARR_ANIME_API_KEY=CHANGEME

# Prowlarr API Key
PROWLARR_API_KEY=CHANGEME

# AdGuard Home API
ADGUARD_USERNAME=admin
ADGUARD_PASSWORD=CHANGEME

# Optional: Custom Grafana Admin Password
# GF_SECURITY_ADMIN_PASSWORD=CHANGEME

